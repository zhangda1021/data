 365  *-------------------------------------------------------------------
 366  *       ACCOUNTING MODEL IN MPSGE TO ILLUSTRATE IDENTITIES
 367  *       IN SAM TABLES
 368  *-------------------------------------------------------------------
 369   
      $model:china_accmodel
       
       
      $sectors:
              Y(g,r)$vom(g,r)                              ! Sectoral production
       and final demands
              A(i,r)$vam(i,r)                              ! Armington composite
       supply
              DX(i,r)$vxm(i,r,"dtrd")                      ! Domestic exports
              FX(i,r)$vxm(i,r,"ftrd")                      ! International expor
      ts
       
      $commodities:
              P(g,r)$(vom(g,r)+vam(g,r))                   ! Domestic supply
              PF(f,r)                                      ! Factor prices
              PTAX(g,r)$(vtax(g,r)-vsub(g,r))              ! Tax payments
              PEX(i)                                       ! Price of domestic e
      xports
              PFX                                          ! Price for foreign e
      xchange
       
      $consumers:
              RH(r)                                        ! Representative hous
      ehold
              GOVT(r)$vom("lg",r)                          ! Government entity b
      y province comprising local and central government activity
              INVS(r)$vdepr("cap",r)                       ! Investment account
       
       
      * SECTORAL PRODUCTION AND FINAL DEMANDS:
      $prod:Y(g,r)$vom(g,r)
              o:P(g,r)                                q:vom(g,r)
              o:PTAX(g,r)                             q:vsub(g,r)
              i:P(i,r)                                q:vafm(i,g,r)
              i:PF(f,r)                               q:vfm(f,g,r)
              i:PTAX(g,r)                             q:vtax(g,r)
       
      * DOMESTIC EXPORTS:
      $prod:DX(i,r)$vxm(i,r,"dtrd")
              o:PEX(i)                       q:vxm(i,r,"dtrd")
              i:P(i,r)                       q:vxm(i,r,"dtrd")
       
      * FOREIGN EXPORTS
      $prod:FX(i,r)$vxm(i,r,"ftrd")
              o:PFX                          q:vxm(i,r,"ftrd")
              i:P(i,r)                       q:vxm(i,r,"ftrd")
       
      * ARMINGTON AGGREGATION FOR DOMESTIC AND IMPORTED VARITIES:
      $prod:A(i,r)$vam(i,r)
              o:P(i,r)                       q:vam(i,r)
              i:PEX(i)                       q:vim(i,r,"dtrd")
              i:P("tp",r)                    q:vtrdm(i,r)
              i:PFX                          q:vim(i,r,"ftrd")
       
      * Investment
      $demand:INVS(r)
              d:P("i",r)                     q:vdepr("cap",r)
              e:PF("cap",r)                  q:vdepr("cap",r)
       
      * HOUSEHOLD INCOME BY INCOME CLASS:
      $demand:RH(r)
              d:P("c",r)       q:vom("c",r)
      * hf --- factor income net of depreciation
              e:PF(f,r)        q:evom(f,r)
      * hg2 --- transfers from government
              e:PFX            q:(vttrn(r,"lg","c"))
      * dhr + hr - drh - rh --- trade deficit:
              e:PFX            q:(-sum(trd,vtrdbal(r,trd,"s")-vtrdbal(r,trd,"d")
      ))
      * psv1 -- investment
              e:P("i",r)       q:(-vinvs(r,"c"))
      * psv2 --- inventory net additions:
              e:P(i,r)         q:(-(vinvta(i,r)-vinvtd(i,r)))
       
       
      * GOVERNMENT ENTITY BY PROVINCE:
      $demand:GOVT(r)$vom("lg",r)
      * g2d --- government consumption
              d:P("lg",r)       q:vom("lg",r)
       
      * "Central government" portion of income:
      *   g2g1 --- transfers paid to local governments
              e:PFX             q:(-vttrn(r,"cg","lg"))
      *   g1g2 --- tranfer income from local governments
              e:PFX             q:vttrn(r,"lg","cg")
      *   cg1s --- income adjustment for central government
              e:P("i",r)        q:vinvsn(r,"cg")
      *   g1sv --- investment adjustment for central government
              e:P("i",r)        q:(-vinvs(r,"cg"))
       
      * "Local government" portion of income:
      *   g2g1 --- transfer income from central government
              e:PFX             q:vttrn(r,"cg","lg")
      *   hg2 --- transfers to household
              e:PFX             q:(-vttrn(r,"lg","c"))
      *   g1g2 --- tranfers paid to central government
              e:PFX             q:(-vttrn(r,"lg","cg"))
      *   tr --- tax revenue from local government
              e:PTAX(i,r)      q:(vtax(i,r)-vsub(i,r))
       
