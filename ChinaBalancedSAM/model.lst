 276  *       Factor payments by sector:
 277  vfm(f,i,r) = sum(mapf(j_,f),sum(mapic(i_,i),sam(r,j_,i_)));
 278  *display vfm;
 279   
 280  *       Intermediate demands for Armington goods:
 281  vafm(j,i,r) =  sum((mapic(i_,i),mapjc(j_,j)),sam(r,j_,i_));
 282  vafm(j,"c",r) =  sum((maph(i_,g),mapjc(j_,j)),sam(r,j_,i_));
 283  vafm(j,"i",r) =  sum((mapinv(i_,g),mapjc(j_,j)),sam(r,j_,i_));
 284  vafm(j,gov,r) =  sum((mapg(i_,gov),mapjc(j_,j)),sam(r,j_,i_));
 285   
 286  *       Domestic and foreign exports and immports:
 287  vxm(i,r,"dtrd")  = sum(mapjc(i_,i),sam(r,i_,"70"));
 288  vxm(i,r,"ftrd")  = sum(mapjc(i_,i),sam(r,i_,"71"));
 289   
 290  vim(i,r,"dtrd")  = sum(mapjc(i_,i),sam(r,"70",i_));
 291  vim(i,r,"ftrd")  = sum(mapjc(i_,i),sam(r,"71",i_));
 292   
 293  *       Output/subsidy taxes:
 294  vtax(i,r) = sum(mapic(i_,i),sam(r,"66",i_));
 295  vsub(i,r) = sum(mapic(i_,i),sam(r,i_,"66"));
 296   
 297  *       Trade margins:
 298  vtrdm(i,r) = sum(mapjc(i_,i),sam(r,"74",i_));
 299   
 300  *       Inventory additions and deletions:
 301  vinvtd(i,r) = sum(mapjc(i_,i),sam(r,"73",i_));
 302  vinvta(i,r)  = sum(mapjc(i_,i),sam(r,i_,"73"));
 303   
 304  *       Transport margins:
 305  vtrnm(i,r) = sum(mapjc(i_,i),sam(r,i_,"74"));
 306   
 307  *       Earnings from factor supply:
 308  evom(f,r) = sum(mapf(j_,f),sam(r,"63",j_));
 309  vdepr(f,r) = sum(mapf(j_,f),sam(r,"72",j_));
 310   
 311  *       Balance of payments surplus:
 312  vtrdbal(r,"dtrd","s") = sam(r,"70","63");
 313  vtrdbal(r,"ftrd","s") = sam(r,"71","63");
 314  vtrdbal(r,"dtrd","d") = sam(r,"63","70");
 315  vtrdbal(r,"ftrd","d") = sam(r,"63","71");
 316   
 317  *       Investment demand by agent:
 318  vinvs(r,"c") = sam(r,"72","63");
 319  vinvs(r,"cg") = sam(r,"72","64");
 320  display vinvs;
 321   
 322  *       Inventory change in household account:
 323  vinvch(r) = sam(r,"73","63");
 324   
 325  *       Transfer payments;
 326  vttrn(r,"lg","cg") = sam(r,"64","65");
 327  vttrn(r,"cg","lg") = sam(r,"65","64");
 328  vttrn(r,"lg","c") = sam(r,"63","65");
 329   
 330  *       Tax revenue:
 331  vtaxrev(r,"lg") = sam(r,"65","66");
 332   
 333  *       Negative demand to close investment account (need to revisit!):
 334  vinvsn(r,"cg") =  sam(r,"64","72");
 335   
 336   
 337  *       Compute additional parameter to define model:
 338  vom("c",r) = sum(j,vafm(j,"c",r));
 339  vom(gov,r) = sum(j,vafm(j,gov,r));
 340  vom("i",r) = sum(j,vafm(j,"i",r));
