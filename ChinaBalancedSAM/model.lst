 342  parameter
 343          vam(g,r)     Armginton production;
 344   
 345  vam(i,r) = sum(trd,vim(i,r,trd)) + vtrdm(i,r)
 346  * + (vinvta(i,r)-vinvtd(i,r))
 347  ;
 348   
 349   
 350  *       Check sparsity:
 351   
 352  parameter total_supply,intermediate_demand;
 353  total_supply(i,r) = vom(i,r) + sum(trd,vim(i,r,trd)) + vinvtd(i,r);
 354  intermediate_demand(j,i,r) = vafm(j,i,r);
 355   
 356  parameter chk_vafm;
 357  *chk_vafm(i,g,r) = yes$(vafm(i,g,r) and (not sum(trd,vim(i,r,trd))));
 358  chk_vafm(i,j,r) = yes$(intermediate_demand(i,j,r) and (not total_supply(i,
      r)));
 359   
 360  display chk_vafm;
